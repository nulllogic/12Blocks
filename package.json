{
  "name": "@nulllogic/16grid",
  "description": "Most advanced responsive front-end CSS framework",
  "version": "1.0.1",
  "version_short": "1.0",
  "keywords": [
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "css-framework",
    "framework",
    "web"
  ],
  "scripts": {
    "css": "npm run css-compile",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/ -o dist/css/",
    "dist": "npm run css"
  },
  "devDependencies": {
    "node-sass": "^6.0.1",
    "sass": "^1.44.0"
  },
  "homepage": "https://nulllogic.github.io/16grid",
  "main": ".babelrc.js",
  "style": "dist/16grid.css",
  "sass": "scss/16grid.scss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nulllogic/16grid.git"
  },
  "author": "Vladimir Lukyanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nulllogic/16grid/issues"
  }
}
